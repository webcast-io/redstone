{
  "name": "redstone",
  "version": "0.0.7",
  "description": "A tool for deploying Node.js applications, heavily influenced by Capistrano.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/axisto-live/redstone.git"
  },
  "keywords": [
    "deployment"
  ],
  "authors": ["Axisto Media Ltd <paul.jensen@axisto.com>", "Tom Loudon <tom.loudon@axisto.com>"],
  "license"         : "BSD",
  "readmeFilename"  : "README.md",
  "gitHead"         : "f91751d7c86bfe495faaf4377ac8550182b9035a",
  "devDependencies" : {
    "coffee-script" : "1.6.2",
    "docco"       : "0.6.2",
    "mocha"       : "1.10.0",
    "gently"      : "0.9.2"
  },
  "dependencies"  : {
    "colors"      : "0.6.0-1",
    "sshclient"   : "git://github.com/webcast-io/node-sshclient.git",
    "underscore"  : "1.4.4"
  },
  "scripts"     : {
    "compile"       : "node_modules/.bin/coffee -c -o lib src",
    "compile-docs"  : "node_modules/.bin/docco src/*.coffee",
    "test"          : "NODE_ENV=test node_modules/.bin/mocha --compilers coffee:coffee-script test/**/*"
  }
}
